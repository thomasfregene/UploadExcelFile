@using UploadExcelFile.Models
@model  List<ContactVM>
@{
    ViewBag.Title = "EditById";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edit file By Batch Id</h2>

@if (Model.Count() > 0)
{
    <hr />
    <table class="table table-bordered table-striped table-responsive tabl">
        <tr>
            <td>FirstName</td>
            <td>LastName</td>
            <td>Email</td>
            <td>Telephone</td>
            <td>Mobile</td>
            <td>CompanyID</td>
        </tr>

        @foreach (ContactVM contact in Model)
        {
            <tr>
                <td><input value="@contact.FirstName" readonly/> </td> 
                <td><input value="@contact.LastName" readonly/> </td>
                <td><input value="@contact.Email" /> </td>
                <td><input value="@contact.Telephone" /></td>
                <td><input value="@contact.Mobile" /> </td>
                <td><input value="@contact.CompanyID" readonly/> </td>
            </tr>

        }
    </table>
}
<form asp-controller="ContactBatch" asp-action="EditById" method="Post" enctype="multipart/form-data">
    <div>
        <input type="submit" value="Update" class="btn btn-block btn-primary" />
    </div>

</form>
